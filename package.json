{
  "name": "@ivicevic/openapi-client-generator",
  "version": "2.5.7",
  "description": "Framework-agnostic OpenAPI client generator for Typescript",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChristianIvicevic/openapi-client-generator"
  },
  "homepage": "https://github.com/ChristianIvicevic/openapi-client-generator",
  "bugs": "https://github.com/ChristianIvicevic/openapi-client-generator/issues",
  "license": "MIT",
  "author": "Christian Ivicevic <mail@christian-ivicevic.com>",
  "main": "dist/main/index.js",
  "typings": "types/index.d.ts",
  "module": "dist/module/index.js",
  "bin": "./bin/openapi-client-generator.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint {src,test}/**/*.ts",
    "build": "yarn build:main && yarn build:module",
    "build:main": "ttsc -p tsconfig.main.json",
    "build:module": "ttsc -p tsconfig.module.json",
    "watch": "ttsc -p tsconfig.main.json -w",
    "cm": "cz",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "change-case": "4.1.2",
    "eslint-config-airbnb": "19.0.4",
    "openapi-types": "9.3.1",
    "prettier": "2.5.1",
    "ramda": "0.27.1",
    "tslib": "2.3.1",
    "winston": "3.3.3",
    "winston-console-format": "1.0.8",
    "yaml": "1.10.2",
    "yargs": "17.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@jest/types": "27.4.2",
    "@types/jest": "27.0.3",
    "@types/node": "14.18.3",
    "@types/prettier": "2.4.2",
    "@types/ramda": "0.27.62",
    "@types/yargs": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "@zerollup/ts-transform-paths": "1.7.18",
    "axios": "0.24.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.5.0",
    "eslint-config-airbnb-typescript": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.2",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "next",
    "jest": "27.4.5",
    "pinst": "2.1.6",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "ttypescript": "1.5.13",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "axios": "*",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "bin",
    "dist/main",
    "dist/module",
    "!**/*.js.map",
    "!**/*.test.*",
    "!**/debug.*",
    "types",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "openapi",
    "openapi3",
    "swagger",
    "rest",
    "typescript",
    "generator"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
